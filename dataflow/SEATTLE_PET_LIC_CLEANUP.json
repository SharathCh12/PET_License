{
	"name": "SEATTLE_PET_LIC_CLEANUP",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "readTSV"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Job_id_Param as string",
				"}",
				"source(output(",
				"          {License Issue Date     License Number     Animal's Name     Species     Primary Breed     Secondary Breed     ZIP Code} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 0,",
				"     ignoreNoFilesFound: false) ~> readTSV",
				"readTSV derive({License Issue Date} = toDate({License Issue Date}, 'MMMM dd yyyy'),",
				"          {Secondary Breed} = upper({Secondary Breed}),",
				"          Job_Exec_ID = $Job_id_Param,",
				"          Job_load_date = currentDate()) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          LIC_ISSUE_DATE = {License Issue Date},",
				"          SPECIES = Species,",
				"          {PRIMARY BREED} = {Primary Breed},",
				"          {SECONDARY BREED} = {Secondary Breed},",
				"          {ZIP CODE} = {ZIP Code},",
				"          JOB_EXEC_ID = Job_Exec_ID,",
				"          JOB_LOAD_DATE = Job_load_date",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> sink1"
			]
		}
	}
}